{
  "name": "analyser",
  "version": "1.0.2",
  "description": "compiler-app",
  "main": "app/main.js",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "danil.sord",
