  "license": "ISC",
  "dependencies": {
    "url-loader": "^4.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "electron": "^11.3.0",
    "electron-builder": "^22.4.1",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
